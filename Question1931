N = int(input())
startTime = []
endTime = []
for i in range(N):
    temp = list(map(int, input().split()))
    startTime.append(temp[0])
    endTime.append(temp[1])

cnt = 0 ## 회의 잡을 수 있는 갯수
while(len(endTime) > 0): # 회의 가능 일정을 하나씩 지우면서 진행. 다 지우면 종료
    if(endTime.count(min(endTime)) == 1):
        i = 0
        while(i < len(endTime)): ## endTime의 최소값 찾음. cnt 추가
            if(endTime[i] == min(endTime)):
                temp = endTime[i] ## 중복 회의 일정 삭제에서 사용할 회의종료시간
                cnt += 1
                del endTime[i]
                del startTime[i] ## 해당 일정 삭제
                break
            i += 1
    else:
        j = 0
        while(j < len(startTime)): ## 동시에 끝나는게 2개 이상 있으므로 제일 먼저 시작하는 회의부터 시작해준다
            if(startTime[j] == min(startTime)):
                temp = endTime[j]  ## 중복 회의 일정 삭제에서 사용할 회의종료시간
                cnt += 1
                del endTime[j]
                del startTime[j]  ## 해당 일정 삭제
                break
            j += 1

    i = 0
    while(i < len(endTime)): ## 중복되는 회의 일정 삭제
        if(startTime[i] < temp):
            del startTime[i]
            del endTime[i]
            i -= 1
        i += 1
print(cnt)
