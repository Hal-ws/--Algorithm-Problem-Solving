N = int(input())

board = [[0 for i in range(N)] for i in range(N)]

def checkpossible(newQueen, queens): ## newQueen의 좌표값을 받음
    for i in range(len(queens)):
        if queens[i] == newQueen[1] or newQueen[0] - queens.index(queens[i]) == newQueen[1] - queens[i]:
            return False
    return True

