import sys

N = int(sys.stdin.readline())
A = list(map(int, sys.stdin.readline().split()))
A_ = A

A_ = sorted(A_)
P = [0] * N
print('A: ' +str(A))
print('A_: ' + str(A_))
for i in range(N):
    P[i] = A_.index(A[i])

B = [0] * N
print('P: ' + str(P))
for i in range(N):
    B[P[i]] = A[i]
print('B: ' + str(B))
